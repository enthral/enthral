{
    "version": "0.1.0",
    "command": "haxe",
    "args": [
        "enthralerFrame.hxml",
        "--next", "enthralerFrame.hxml",
        "--next", "enthralerEditor.hxml",
        "--next", "enthralerHost.hxml",
        "--next", "apiDocs.hxml",
        "--next", "test.hxml",
        "--next", "client_enthralerdotcom.hxml",
        "--next", "server_enthralerdotcom.hxml"
    ],
    "problemMatcher": {
        "owner": "haxe",
        "pattern": {
            "regexp": "^(.+):(\\d+): (?:lines \\d+-(\\d+)|character(?:s (\\d+)-| )(\\d+)) : (?:(Warning) : )?(.*)$",
            "file": 1,
            "line": 2,
            "endLine": 3,
            "column": 4,
            "endColumn": 5,
            "severity": 6,
            "message": 7
        }
    }
}
